/* pages/index/more/steamfic/steamfic.wxss */
page{
  background-color: white;
}

.page .flex-item{
  display: flex;
  margin: 30rpx 20rpx;
}
.page .flex-item text{
  width: 260rpx;
  line-height: 64rpx;
  font-size: 30rpx;
}
.page .flex-item .flexInputbox{
  display: flex;
  justify-content: space-between;
  align-items: center;
  border: 2rpx solid #656565;
  border-radius: 20rpx;
  padding: 5rpx 0;
}
.page .flex-item .flex-input{
  width: 275rpx;
  font-size: 30rpx;
  padding-left: 10rpx;
}
.page .flex-item .flexInputbox .text{
  border-left:2rpx solid #c1c1c1;
  width: 90rpx;
  font-size: 30rpx;
  text-align: center;
  height: 40rpx;
  line-height: 40rpx;
  margin-top:5rpx; 
}
.section-mt{
  margin-top: 60rpx;
}
.page .img{
  width: 98%;
  margin: 0 auto;
}
.page .img .section__title{
  margin: 60rpx 0;
}
.jsBox{
  display: flex;
  justify-content: space-between;
}
.page .img .img-box{
  width: 82%;
  margin: 0 auto;
  height: 100rpx;
}
.page .img .img-box image{
  width: 100%;
  height: 100%;
}
